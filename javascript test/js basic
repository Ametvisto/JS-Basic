}

let names = ["giorgi", "caterina", "Khuanito", "Chikitina", "Eke"];

for (let i = 0; i < names.length; i++) {
    if (names[i].length > 3) {
        console.log(names[i])
    }
}

let numbers = ["12", "13", "245", "54", "0", "63", "34", "7", "8"];

for (let i = 0; i < numbers.length; i++) {
    if (numbers[i] % 2 == 1) {
        console.log(numbers[i]);
    }
}

